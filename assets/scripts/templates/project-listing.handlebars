{{#each projects as |project|}}
  {{log project}}

  <div>
    <h1 data-id="{{project.id}}"> <small>{{ project.name }}</small></h1>
    <section data-id="{{project.id}}">
      <p>
        {{project.description}}
      </p>
      <button type="submit" class="btn btn-default delete-book">Remove</button>
    </section>
  </div>

{{/each}}
